model Cooling {
  id            String      @id @default(uuid())
  Rgb           Boolean
  CoolingMarkId String
  CoolingTypeId String
  FansNumberId  String
  CPUSupportId  String
  CPUSupport    CPUSupport  @relation(fields: [CPUSupportId], references: [id])
