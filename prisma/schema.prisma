model PCcaseBrand {
  id     String   @id @default(uuid())
  name   String
  pccase PCcase[]
}

model PCcaseCaseformat {
  id     String   @id @default(uuid())
  name   String
  pccase PCcase[]
}

model PCcaseNumberofFansPreinstalled {
  id     String   @id @default(uuid())
  name   String
  pccase PCcase[]
